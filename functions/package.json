{
  "name": "functions",
  "scripts": {
    "sync:test": "cross-env NODE_NO_WARNINGS=1 ts-node src/scripts/syncAllTest.ts",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts . --fix",
    "build": "rimraf lib && tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && cross-env NODE_NO_WARNINGS=1 firebase emulators:start --only functions,firestore",
    "shell": "npm run build && cross-env NODE_NO_WARNINGS=1 firebase functions:shell",
    "start": "npm run shell",
    "deploy": "cross-env NODE_NO_WARNINGS=1 firebase deploy --only functions",
    "logs": "cross-env NODE_NO_WARNINGS=1 firebase functions:log",
    "cleanup:elements": "ts-node src/scripts/cleanupElements.ts",
    "sync:groups": "ts-node src/scripts/syncGroups.ts",
    "sync:cards": "ts-node src/scripts/syncCards.ts",
    "sync:cards:force": "ts-node src/scripts/syncCards.ts --force",
    "sync:cards:group": "ts-node src/scripts/syncCards.ts --group",
    "sync:cards:group:force": "ts-node src/scripts/syncCards.ts --group --force",
    "sync:square-enix": "ts-node src/scripts/storeSquareEnixCards.ts",
    "sync:square-enix:force": "ts-node src/scripts/storeSquareEnixCards.ts --force",
    "sync:update-cards": "ts-node src/scripts/updateCardsWithSquareEnixData.ts",
    "sync:update-cards:force": "ts-node src/scripts/updateCardsWithSquareEnixData.ts --force",
    "sync:all": "ts-node src/scripts/syncAll.ts",
    "cleanup:sets": "ts-node src/scripts/cleanupSetsField.ts",
    "sync:all:force": "ts-node src/scripts/syncAll.ts --force",
    "sync:all:group": "ts-node src/scripts/syncAll.ts",
    "sync:all:group:force": "ts-node src/scripts/syncAll.ts --force",
    "sync:search-index": "ts-node src/scripts/updateSearchIndex.ts",
    "metadata:init": "ts-node src/scripts/initializeMetadata.ts",
    "metadata:view": "ts-node src/scripts/viewMetadata.ts"
  },
  "engines": {
    "node": "22"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.485.0",
    "@aws-sdk/s3-request-presigner": "^3.485.0",
    "@types/node-fetch": "^2.6.12",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^6.5.0",
    "lru-cache": "^7.14.1",
    "minimist": "^1.2.8",
    "node-fetch": "^3.3.2",
    "punycode": "^2.3.1",
    "soundex-code": "^2.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/minimist": "^1.2.5",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.1.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.0"
  },
  "private": true,
  "overrides": {
    "punycode": "^2.3.1"
  },
  "publishConfig": {
    "registry": "https://us-central1-npm.pkg.dev/fftcg-sync-service/gcf-artifacts/"
  }
}
