{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts . --fix",
    "build": "rimraf lib && tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions,firestore",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "cleanup:elements": "ts-node src/scripts/cleanupElements.ts",
    "sync:groups": "ts-node src/scripts/syncGroups.ts"
  },
  "engines": {
    "node": "22"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.485.0",
    "@aws-sdk/s3-request-presigner": "^3.485.0",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^6.3.0",
    "lru-cache": "^7.14.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.1.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.0"
  },
  "private": true,
  "publishConfig": {
    "registry": "https://us-central1-npm.pkg.dev/fftcg-sync-service/gcf-artifacts/"
  }
}
